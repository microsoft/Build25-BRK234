{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "name": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "useStrictRaiPolicy": {
        "value": "${USE_STRICT_RAI_POLICY=false}"
      },
      "useKeylessAuth": {
        "value": "${USE_KEYLESS_AUTH=false}"
      },
      "useVnet": {
        "value": "${USE_VNET=false}"
      },
      "usePrivateIngress": {
        "value": "${USE_PRIVATE_INGRESS=false}"
      },
      "useLogin": {
        "value": "${USE_LOGIN=false}"
      },
      "useMonitoring": {
        "value": "${USE_MONITORING=false}"
      },
      "useFrontDoor":{
        "value": "${USE_FRONT_DOOR=false}"
      },
      "acaExists": {
        "value": "${SERVICE_ACA_RESOURCE_EXISTS=false}"
      },
      "runningOnGh": {
        "value": "${GITHUB_ACTIONS}"
      }
    }
  }
